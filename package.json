{
  "name": "@depixy/altair",
  "version": "2.0.0",
  "description": "",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/cjs/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module node16 --outDir lib/esm",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "lint": "eslint --ext .ts src"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "keywords": [
    "depixy",
    "depixy-plugin"
  ],
  "homepage": "https://github.com/depixy/altair#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/depixy/altair.git"
  },
  "bugs": {
    "url": "https://github.com/depixy/altair/issues"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "@fastify/static": "^5.0.2",
    "altair-static": "^4.4.2",
    "fastify-plugin": "^3.0.1"
  },
  "devDependencies": {
    "@joshuaavalon/eslint-config-typescript": "^6.1.4",
    "@types/node": "^16.11.39",
    "eslint": "^8.17.0",
    "fastify": "^3.29.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  }
}
